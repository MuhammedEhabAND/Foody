<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TitleTextView" parent="ThemeOverlay.Material3.AutoCompleteTextView">
        <item name="android:layout_marginStart">10dp</item>
        <item name="fontFamily">sans-serif-medium</item>
        <item name="android:layout_marginTop">10dp</item>
        <item name="android:textSize">32sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/light_mode_text_color</item>

    </style>
    <style name="SearchBarStyle"
        parent="@style/Widget.Material3.SearchBar">
        <item name="android:background">@drawable/search_bar_background_light</item>

        <item name="android:textColorHint">#6B000000</item>
        <item name="android:elevation">6dp</item>
        <item name="android:textColor">@color/black</item>
        </style>
</resources>